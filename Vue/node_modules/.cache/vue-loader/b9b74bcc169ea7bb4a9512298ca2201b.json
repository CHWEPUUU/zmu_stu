     },\r\n      //更新编辑内容\r\n      update(){\r\n        //post传对象\r\n        this.$axios.post('/student/edit',this.form)   \r\n      }\r\n    },\r\n    //获取课程信息\r\n    beforeMount () {\r\n      this.$axios.post('/course_list').then(\r\n        response => {\r\n          this.tableData = response.data\r\n        },\r\n        error => {\r\n          console.log(error.message)\r\n        })\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]}]}