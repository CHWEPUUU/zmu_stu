ot=\"footer\" class=\"dialog-footer\">\r\n      <!-- 多个事件用;隔开 -->\r\n      <el-button type=\"primary\" @click=\"dialogFormVisible = false;Update()\">确 定</el-button>\r\n      <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n    </div>\r\n  </el-dialog>  \r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n    name: \"CourseInfo\",\r\n    data() {\r\n    return {\r\n      dialogFormVisible: false,\r\n      form: {\r\n        sdept: '',\r\n        sname: '',\r\n        grade: ''      \r\n      },\r\n      sc:{\r\n        sno: '',\r\n        cno: '',\r\n        grade: '',\r\n        status:null\r\n      },\r\n      student:{\r\n        sno: '',\r\n        sname: '',\r\n        sdept:''\r\n      },\r\n      formLabelWidth: '120px',\r\n      update: false,\r\n    }\r\n  },\r\n  methods: {\r\n    Update(){\r\n        this.sc.grade = this.form.grade\r\n        this.student.sdept = this.form.sdept\r\n\r\n        // grade和sdept分别属于两张不同的表,需要异步操作\r\n        let updateData = async () => {\r\n            try {\r\n                //更新grade\r\n                await this.$axios.post('/update_SC',this.sc);\r\n                //更新sdept\r\n                await this.$axios.post('/update_student',this.student);\r\n            } catch (error) {              \r\n                console.error(error);\r\n            }\r\n        }\r\n        // 调用updateData函数\r\n        updateData();\r\n    }\r\n  },\r\n    mounted() {\r\n        this.$bus.$on('formCS',(data)=>{\r\n            this.dialogFormVisible = true,\r\n            this.form = data\r\n\r\n            this.sc.sno = data.sno\r\n            this.student.sno = data.sno           \r\n            this.student.sname = data.sname\r\n        })\r\n        this.$bus.$on('form_cno',(data)=>{\r\n            this.sc.cno = data          \r\n        })\r\n    }, \r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAwBA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,iBAAA;MACAC,IAAA;QACAC,KAAA;QACAC,KAAA;QACAC,KAAA;MACA;MACAC,E